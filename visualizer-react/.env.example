# Environment variables for the Fastify server
# Copy this file to .env and adjust as needed

# Node environment: development | production
NODE_ENV=development

# Fastify server port
PORT=5178

# Fastify/Pino log level: fatal | error | warn | info | debug | trace | silent
LOG_LEVEL=info

# Icon cache limits
ICON_MEM_LRU_MAX_ENTRIES=512
ICON_MEM_LRU_MAX_MB=64
ICON_DISK_CACHE_MAX_MB=100
